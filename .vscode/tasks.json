{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build hello world", //launch.json中preLaunchTask一致
            "type": "shell",
            "command": "g++",
            "args": [
                "-fopenmp",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "group": "build"
        }
    ]
}